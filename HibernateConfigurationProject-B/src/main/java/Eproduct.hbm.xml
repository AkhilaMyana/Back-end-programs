<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="p1.Eproduct" table="eproduct1">
		<id name="id" column ="id"></id>
		<property name="name" >
		       <column name="name"/>
		 </property>
        <property name="price">
            <column name="price"/>
         </property>
        <property name="date_added">
        	<column name="date_added"/>
        </property>
	</class>
</hibernate-mapping>